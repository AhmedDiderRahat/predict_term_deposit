{
  "algorithm": "SVC_CLASSIFICATION",
  "svc_grid": {
    "C": {
      "gridMode": "EXPLICIT",
      "randomMode": "RANGE",
      "limit": {
        "min": 1.0E-23
      },
      "values": [
        1.0
      ],
      "range": {
        "min": 0.1,
        "max": 10.0,
        "nbValues": 3,
        "scaling": "LOGARITHMIC"
      }
    },
    "gamma": {
      "values": {
        "auto": {
          "enabled": false
        },
        "custom": {
          "enabled": false
        },
        "scale": {
          "enabled": true
        }
      }
    },
    "custom_gamma": {
      "gridMode": "EXPLICIT",
      "randomMode": "RANGE",
      "limit": {
        "min": 1.0E-23
      },
      "values": [
        0.001
      ],
      "range": {
        "min": 1.0E-4,
        "max": 1.0,
        "nbValues": 3,
        "scaling": "LOGARITHMIC"
      }
    },
    "kernel": {
      "values": {
        "linear": {
          "enabled": false
        },
        "poly": {
          "enabled": false
        },
        "rbf": {
          "enabled": true
        },
        "sigmoid": {
          "enabled": false
        }
      }
